#include <stdio.h>

int main() {
   
  int row;
  printf("enter the row size:-");
  scanf("%d",&row);
  
  int column;
  printf("enter the column size:-");
  scanf("%d",&column);
  
  int i,j,s1,s2;
  int arr[row][column];
  for(i=0;i<row;i++){
      for(j=0;j<column;j++)
      scanf("%d",&arr[i][j]);
  }
   for(i=0;i<row;i++){
      for(j=0;j<column;j++)
      s1=s1+arr[i][j];
      s2=s2+arr[i][j];
  }
  printf(" s1=%d,s2=%d",s1,s2);

    return 0;
}
