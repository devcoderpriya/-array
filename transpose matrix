#include<stdio.h>
int main(){
    int row;
    printf("enter the row size:-");
    scanf("%d",&row);

    int column;
    printf("enter the column size:-");
    scanf("%d",&column);
    int arr[row][column];
    int i,j;
    for(i=0;i<row;i++){
        for(j=0;j<column;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    for(i=0;i<column;i++){
        for(j=0;j<row;j++)
        {
            printf("%d",&arr[i][j]);
        }
    }
    return 0;
    }
